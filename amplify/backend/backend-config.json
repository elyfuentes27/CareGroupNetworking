{
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"CGAppApi": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "hosting",
					"resourceName": "S3AndCloudFront",
					"attributes": [
						"HostingBucketName"
					]
				}
			]
		},
		"n": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		}
	},
	"api": {
		"CGAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "CGAppApi",
					"attributes": [
						"Name",
						"Arn"
					]
				},
				{
					"category": "function",
					"resourceName": "n",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}